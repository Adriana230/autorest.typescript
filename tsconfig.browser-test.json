{
  "compilerOptions": {
    "skipLibCheck": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": false,
    "module": "commonjs",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": false,
    "declarationMap": false,
    "strict": true,
    "declaration": true,
    "stripInternal": true,
    "noEmitHelpers": false,
    "target": "es2018",
    "types": ["node", "mocha"],
    "lib": ["es2018", "DOM"],
    "experimentalDecorators": true,
    "newLine": "LF",
    "outDir": "./test/integration",
    "rootDir": "./test/integration"
  },
  "include": ["test/integration/*.ts", "test/integration/generated/**/*.ts"],
  "exclude": []
}

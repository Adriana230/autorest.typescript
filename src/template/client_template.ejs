import * as coreHttp from "@azure/core-http";
import * as Models from "./models";
import * as Mappers from "./models/mappers";
import * as operations from "./operations";
import { <%= client.clientContextClassName %> } from "./<%= client.clientContextFileName %>";

class <%= client.clientClassName %> extends <%= client.clientContextClassName %> {
  // Operation groups
  <% client.operationGroupsNameMapper.forEach(og => { %>
    <%= og.operationGroupReferenceName %>: operations.<%= og.operationGroupName %>;
  <% }); %>

  /**
   * Initializes a new instance of the <%= client.clientClassName %> class.
   * @param url The URL of the service account, container, or blob that is the targe of the desired
   * operation.
   * @param [options] The parameter options
   */
  constructor(url: string, options?: coreHttp.ServiceClientOptions) {
    super(url, options);
    <% client.operationGroupsNameMapper.forEach(og => { %>
    this.<%= og.operationGroupReferenceName %> = new operations.<%= og.operationGroupName %>(this);
    <% }); %>
  }
}

// Operation Specifications

export {
  <%= client.clientClassName %>,
  <%= client.clientContextClassName %>,
  Models as <%= client.modelsName %>,
  Mappers as <%= client.mappersName %>
};
export * from "./operations";